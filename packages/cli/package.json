{
  "name": "@nativecn/cli",
  "version": "0.3.0",
  "private": false,
  "description": "CLI tool for initializing and managing NativeCN components in React Native projects",
  "author": "major tom <tailwiinder@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/tailwiinder/nativecn#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tailwiinder/nativecn.git"
  },
  "keywords": [
    "react-native",
    "nativecn",
    "ui-components",
    "cli"
  ],
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "nativecn": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x dist/index.js",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "clsx": "^2.0.0",
    "commander": "^10.0.0",
    "execa": "^5.1.1",
    "fs-extra": "^11.0.0",
    "nativewind": "^2.0.0",
    "prompts": "^2.4.2",
    "react": "^18.0.0",
    "react-native": "^0.72.0",
    "react-native-vector-icons": "^9.2.0",
    "tailwind-merge": "^3.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/prompts": "^2.4.2",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.72.0",
    "@types/react-native-vector-icons": "^6.4.13",
    "typescript": "^5.8.2"
  }
}
